{
  "name": "npm-utils",
  "description": "Async NPM shell commands",
  "version": "0.0.0-semantic",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": {
    "url": "https://github.com/bahmutov/npm-utils/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "grunt",
        "npm test",
        "npm version"
      ],
      "pre-push": [],
      "post-commit": [
        "npm run exact-semver"
      ],
      "post-merge": []
    }
  },
  "contributors": [
    "Jayson Harshbarger http://hypercubed.com/"
  ],
  "czConfig": {
    "path": "node_modules/pre-git/node_modules/cz-conventional-changelog"
  },
  "dependencies": {
    "check-more-types": "2.1.2",
    "lazy-ass": "0.6.0",
    "q": "1.4.1",
    "verbal-expressions": "0.1.2",
    "win-spawn": "2.0.0"
  },
  "devDependencies": {
    "codacy-coverage": "1.1.3",
    "exact-semver": "1.2.0",
    "git-issues": "1.2.0",
    "grunt": "0.4.5",
    "grunt-bump": "0.6.0",
    "grunt-complexity": "0.3.0",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-filenames": "0.4.0",
    "grunt-nice-package": "0.9.4",
    "gt": "0.8.49",
    "matchdep": "1.0.0",
    "pre-git": "1.3.0",
    "semantic-release": "4.3.5"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "homepage": "https://github.com/bahmutov/npm-utils",
  "keywords": [
    "async",
    "command",
    "npm",
    "shell"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/npm-utils.git"
  },
  "scripts": {
    "commit": "git-issues && commit-wizard",
    "coverage-codacy": "cat cover/lcov.info | ./node_modules/.bin/codacy-coverage -v -d",
    "exact-semver": "exact-semver",
    "issues": "git-issues",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "gt src/test/*.js --output"
  }
}
